<div id="content">
  <div id="grid-container">
    <h2><%= header01 %></h2>
    <div id="grid">
      <% for (let i = 0; i < events.length; i += 1) { %>
        <div class="grid-item">
          <a href="/event?idEvent=<%= events[i].id %>">
            <div class="container">
              <div class="image">
                <img src="images/uploads/<%- events[i].image %>">
              </div>
              <div class="title">
                <h3>
                  <%= events[i].name %>
                </h3>
              </div>
            </div>
          </a>
          <p>
            <span><%= events[i].date %></span>
          </p>
          </div>
      <% } %>
    </div>
  </div>
</div>